unsigned long countTime = 0;

// Push button
int buttonState;
int pushCounter = 0;
int buttonPin = 7;
int lastButtonState = LOW; // is not push with current set up

void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  randomSeed(analogRead(0)); // Initialize random seed
  if(!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }
	pinMode(buttonPin,INPUT_PULLUP);
}



void loop() {
  // put your main code here, to run repeatedly:
   buttonState = digitalRead(buttonPin);
	 if(lastButtonState != buttonState ){
		if(!isPlaying){
    		pushCounter++;
		}
    lastButtonState = buttonState;
  }
  
  if(pushCounter == 2 ){
		isPlaying = true;
		play();
		if(!isPlaying){
			reset();
		}
  } 
			
}

void reset(){
	timeCounter = -1; 
	pushCounter = 0; 
	isPlaying = false;
	shakeTime = 0;
	countTime = 0;
	frame = 0;
}

void play(){
	display.clearDisplay();
	unsigned long currentTime = millis();
	if(isPlaying){
			int frameLength = 0;
			// Counting
			if(getGameStage() == 0){
				if (currentTime - shakeTime >= FRAME_DELAY ) {			
					drawAnimation (-1 , frame);
					drawNumber(timeCounter);
					display.display();
					frameLength =  getFrameLength(-1);
					frame = (frame + 1) % frameLength;
					shakeTime = currentTime;
  		}

				if (currentTime - countTime >= COUNTER_DELAY ) {		
					timeCounter++;
					countTime = currentTime;		
				}

			}
			

			
			if(getGameStage() == 1){
					clearNumberScreen();
					display.drawBitmap(num_spriteX, num_spriteY, go, FRAME_NUM_WIDTH, FRAME_NUM_HEIGHT, 1 );

					frame = 0;
					int randomNum = random(3); // 1 : rock , 2: paper, 3: scissor
					frameLength =  getFrameLength(randomNum);
					do{
								// clear display
								clearAnimationScreen();
								drawAnimation (randomNum, frame);
								display.display();
								
								frame = (frame + 1) % frameLength;
								delay(FRAME_DELAY);
						} while (frame < frameLength - 1);

					isPlaying = false;
			
	}
}
}

int getFrameLength (int indicatedNum) {
	switch (indicatedNum){
		case -1 : return fist_num_frames ; break;
		case 0 : return rock_num_frames ; break;
		case 1 : return paper_num_frames; break;
		case 2 : return scissor_num_frames; break;
	}
}

void drawAnimation(int indicatedNum , int frame){
	//Clear the number first 
	switch (indicatedNum){
		case -1 : display.drawBitmap(ani_spriteX, ani_spriteY, fist_frames[frame], FRAME_ANIMATOR_WIDTH, FRAME_ANIMATOR_HEIGHT, 1 ); break;
		case 0 : display.drawBitmap(ani_spriteX, ani_spriteY, rock_frames[frame], FRAME_ANIMATOR_WIDTH, FRAME_ANIMATOR_HEIGHT, 1 ); break;
		case 1 : display.drawBitmap(ani_spriteX, ani_spriteY, paper_frames[frame], FRAME_ANIMATOR_WIDTH, FRAME_ANIMATOR_HEIGHT, 1 ); break;
		case 2 : display.drawBitmap(ani_spriteX, ani_spriteY, scissor_frames[frame], FRAME_ANIMATOR_WIDTH, FRAME_ANIMATOR_HEIGHT, 1 ); break;
	}

}

void drawNumber(int counter){
	//Clear the number first 
	clearNumberScreen();
	if(counter == 0){
		display.drawBitmap(num_spriteX, num_spriteY, num_3, FRAME_NUM_WIDTH, FRAME_NUM_HEIGHT, 1 );
	} else if(counter == 1){
		display.drawBitmap(num_spriteX, num_spriteY, num_2, FRAME_NUM_WIDTH, FRAME_NUM_HEIGHT, 1 );
	}else if(counter == 2){
		display.drawBitmap(num_spriteX, num_spriteY, num_1, FRAME_NUM_WIDTH, FRAME_NUM_HEIGHT, 1 );
	}
}

void clearNumberScreen(){
	display.fillRect(num_spriteX, num_spriteY,FRAME_NUM_WIDTH, FRAME_NUM_HEIGHT, BLACK);
}

void clearAnimationScreen(){
	display.fillRect(ani_spriteX, ani_spriteY,FRAME_ANIMATOR_WIDTH, FRAME_ANIMATOR_HEIGHT, BLACK);
}

int getGameStage(){
	if(timeCounter < 3){
		return 0; // counting stage
	} else {
		return 1; // result stage
	}
}
